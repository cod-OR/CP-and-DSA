#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define pb push_back()

std::vector<ll> ar(10001);
std::vector<ll> seg_tree(40004);

void built(int st, int en, int i){
	if(st==en)seg_tree[i]=ar[st];
	else{
		mid=(st+en)/2;
		build(st,mid,2*i+1);
		build(mid+1,en,2*i+2);
		seg_tree[i]=seg_tree[2*i+1]+seg_tree[2*i+2];
	}
}

void update_query(int x,int y,int i,int st,int en){
	if(en<x || st>x)return;
	if(st==en){
		seg_tree[i]=y;
	}
	else{
		int mid=(st+en)/2;
		update_query(x,y,2*i+1,st,mid);
		update_query(x,y,2*i+2,mid+1,en);
		seg_tree[i]=seg_tree[2*i+1]+seg_tree[2*i+2];
	}
}

int query(int x,int y){
	if(en<x || st>y)return 0;
	if(st>=x && en<=y)return seg_tree[i];

	int mid=(st+en)/2;
	return query(x,y,i*2+1,st,mid) +
			query(x,y,i*2+2,mid+1,en);
}

int main(){
	int n,q;
	cin>>n;
	for(int i=0;i<n;i++){
		cin>>ar[i];
	}
	cin>>q;
	build(0,n-1,0);
	while(q--){
		int c,x,y;
		cin>>c>>x>>y
		if(c==1)update_query(x-1,y,0,0,n-1);
		else{
			cin>>x>>y;
			query(x-1,y-1,0,0,n-1);
		}
	}

}
